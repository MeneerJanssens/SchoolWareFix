{
  "manifest_version": 3,
  "name": "SchoolWare Fix",
  "version": "1.0",
  "description": "Multiple fixes for SchoolWare intranet site.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://kaozele-intranet.durme.be/*"],
      "css": ["styles.css"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
